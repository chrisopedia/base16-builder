$base16 : (
	'00' : rgba(<%= @base["00"]["cssrgba"] %>),
	'01' : rgba(<%= @base["01"]["cssrgba"] %>),
	'02' : rgba(<%= @base["02"]["cssrgba"] %>),
	'03' : rgba(<%= @base["03"]["cssrgba"] %>),
	'04' : rgba(<%= @base["04"]["cssrgba"] %>),
	'05' : rgba(<%= @base["05"]["cssrgba"] %>),
	'06' : rgba(<%= @base["06"]["cssrgba"] %>),
	'07' : rgba(<%= @base["07"]["cssrgba"] %>),
	'08' : rgba(<%= @base["08"]["cssrgba"] %>),
	'09' : rgba(<%= @base["09"]["cssrgba"] %>),
	'0A' : rgba(<%= @base["0A"]["cssrgba"] %>),
	'0B' : rgba(<%= @base["0B"]["cssrgba"] %>),
	'0C' : rgba(<%= @base["0C"]["cssrgba"] %>),
	'0D' : rgba(<%= @base["0D"]["cssrgba"] %>),
	'0E' : rgba(<%= @base["0E"]["cssrgba"] %>),
	'0F' : rgba(<%= @base["0F"]["cssrgba"] %>)
);
